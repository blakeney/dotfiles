# Use Exuberant CTags for gVim compatibility
alias ctags='exctags --exclude=target --exclude=vendor -f .tags'

# Git Aliases
alias Gs='git status'
alias Gl='git log'
alias Gd='git pull'
alias Gf='git diff'
alias Gu='git push'
alias Ga='git add'
alias Gc='git commit'
alias Gca='git commit -a'
alias Go='git checkout'
alias Gb='git branch'
alias Gr='git remote'
